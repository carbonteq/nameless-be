{
	"$schema": "https://biomejs.dev/schemas/1.8.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"javascript": {
		"parser": { "unsafeParameterDecoratorsEnabled": true }
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"style": {
				"useImportType": { "fix": "none", "level": "off" }
			}
		}
	}
}
